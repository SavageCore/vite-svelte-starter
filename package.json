{
  "name": "vite-svelte-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:remote": "vite --host",
    "build": "pwa-assets-generator && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
    "format": "eslint --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
    "format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@picocss/pico": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@tsconfig/svelte": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.0",
    "prettier-plugin-svelte": "^3.2.8",
    "sass": "^1.81.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.2",
    "svelte": "^5.2.3",
    "svelte-check": "^4.0.9",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-pwa": "^0.21.0",
    "vitest": "^2.1.5"
  }
}