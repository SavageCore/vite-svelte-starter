{
  "name": "vite-svelte-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:remote": "vite --host",
    "build": "pwa-assets-generator && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
    "format": "eslint --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
    "format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@picocss/pico": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/svelte": "^5.2.1",
    "@tsconfig/svelte": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.10.0",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.0",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.77.8",
    "stylelint": "^16.8.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.2",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.6",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-pwa": "^0.20.1",
    "vitest": "^2.0.4"
  }
}